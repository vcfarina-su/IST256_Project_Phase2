    "def Slice(text):\n",
    "    newText = []\n",
    "    for word in text:\n",
    "        if word != word.isalpha():\n",
    "            splitted = word.split\n",
    "    newText.append(splitted)\n",
    "    return newText\n",
    "\n",
    "text = input('Chemicals: ')\n",
    "\n",
    "getChems = Slice(text)\n",
    "\n",
    "print(getChems)\n",
